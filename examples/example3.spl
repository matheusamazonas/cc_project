// A main function to check the results
// It takes no arguments, so the type looks like this:
main ( ) :: -> Void {
	var n = 0;
	var facN = 1;
	var ok = True;
	
	while ( n < 20 ) {
		facN = facR ( n );
		i f ( facN != facI ( n ) || facN != facL ( n )) {
			print ( n : facN : facI ( n ) : facL ( n ) : [] );
			ok = False;
		}
		n = n + 1;
	}
	print ( ok );
}
